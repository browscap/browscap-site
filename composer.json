{
    "name": "browscap/browscap-site",
    "type": "application",
    "description": "Website for the Browser Capabilities project",
    "keywords": ["browscap"],
    "homepage": "https://github.com/browscap/browscap-site",
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/browscap/browscap/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/browscap/browscap-site/issues",
        "source": "https://github.com/browscap/browscap-site"
    },
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "beberlei/assert": "^2.9",
        "browscap/browscap": "6.0.34",
        "browscap/browscap-php": "^4.1",
        "composer/composer": "^1.7",
        "doctrine/cache": "^1.8",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.17",
        "ocramius/package-versions": "^1.3",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0",
        "roave/doctrine-simplecache": "^2.1",
        "slim/slim": "^3.11",
        "slim/twig-view": "^2.4",
        "symfony/console": "^4.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.4",
        "mikey179/vfsstream": "^1.6"
    },
    "config": {
        "process-timeout": 0,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BrowscapSite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowscapSiteTest\\": "tests/"
        }
    },
    "bin": ["bin/browscap-site"],
    "scripts": {
        "post-install-cmd": [
            "bin/browscap-site generate-build"
        ],
        "post-update-cmd": [
            "bin/browscap-site generate-build"
        ]
    }
}
