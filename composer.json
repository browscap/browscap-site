{
    "name": "browscap/browscap-site",
    "type": "application",
    "description": "Website for the Browser Capabilities project",
    "keywords": ["browscap"],
    "homepage": "https://github.com/browscap/browscap-site",
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/browscap/browscap/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/browscap/browscap-site/issues",
        "source": "https://github.com/browscap/browscap-site"
    },
    "require": {
        "php": "7.4.*",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "browscap/browscap": "^6.0",
        "browscap/browscap-php": "^5.0",
        "composer-plugin-api": "^2.1",
        "doctrine/cache": "^1.10",
        "guzzlehttp/guzzle": "^7.4",
        "laminas/laminas-config-aggregator": "^1.7",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-servicemanager": "^3.10",
        "lazypdo/lazypdo": "^2.0",
        "monolog/monolog": "^2.3",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0",
        "roave/doctrine-simplecache": "^2.3",
        "slim/slim": "^4.9",
        "slim/twig-view": "^3.2",
        "symfony/console": "^5.4",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.15"
    },
    "config": {
        "platform": {
            "php": "7.4.26"
        },
        "process-timeout": 0,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BrowscapSite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowscapSiteTest\\": "tests/"
        }
    },
    "bin": ["bin/browscap-site"],
    "scripts": {
        "post-install-cmd": [
            "bin/browscap-site generate-build"
        ],
        "post-update-cmd": [
            "bin/browscap-site generate-build"
        ]
    }
}
