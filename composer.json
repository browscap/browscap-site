{
    "name": "browscap/browscap-site",
    "type": "application",
    "description": "Website for the Browser Capabilities project",
    "keywords": ["browscap"],
    "homepage": "https://github.com/browscap/browscap-site",
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/browscap/browscap/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/browscap/browscap-site/issues",
        "source": "https://github.com/browscap/browscap-site"
    },
    "require": {
        "php": "8.1.*",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "composer-plugin-api": "^2.2",
        "azjezz/psl": "^2.3",
        "browscap/browscap": "^6.1.3",
        "browscap/browscap-php": "^7.2",
        "guzzlehttp/guzzle": "^7.5",
        "laminas/laminas-config-aggregator": "^1.13",
        "laminas/laminas-diactoros": "^3.3",
        "laminas/laminas-servicemanager": "^3.20",
        "lazypdo/lazypdo": "^2.0",
        "league/flysystem": "^3.12",
        "matthiasmullie/scrapbook": "^1.5",
        "monolog/monolog": "^3.2",
        "php-standard-library/psalm-plugin": "^2.2",
        "psr/container": "^1.1",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "slim/slim": "^4.11",
        "slim/twig-view": "^3.3",
        "symfony/console": "^6.2",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "doctrine/coding-standard": "^11.1",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18.4",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.4"
    },
    "config": {
        "platform": {
            "php": "8.1.12"
        },
        "process-timeout": 0,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "BrowscapSite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowscapSiteTest\\": "tests/"
        }
    },
    "bin": ["bin/browscap-site"],
    "scripts": {
        "post-install-cmd": [
            "bin/browscap-site generate-build"
        ],
        "post-update-cmd": [
            "bin/browscap-site generate-build"
        ]
    }
}
